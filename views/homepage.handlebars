<div class="row search-field">
    <div class="col s12">
        <div class="row">
            <div class="input-field col l4">
                <input type="text" id="searchField">
                <label for="searchField">Search</label>
            </div>
            <button class="btn waves-effect waves-light search-btn deep-purple darken-3" type="submit" name="action">
                <i class="material-icons">send</i>
            </button>
        </div>
    </div>
</div>

{{#each groups as |group| }}
<div class="container">
    <div class="row">
        <div class="col s1">Picture</div>
        <div class="col s5">{{content}}</div>
        <div class="col s2 right">Casual</div>
        <div class="col s2 right">3 / 5 People</div>
        <div class="col s2 right">Shooter</div>
    </div>
</div>
    
{{/each}}