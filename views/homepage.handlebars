<div class="row search-field">
    <div class="col s12">
        <div class="row">
            <div class="input-field col l4">
                <input type="text" id="searchField">
                <label for="searchField">Search</label>
            </div>
            <button class="btn waves-effect waves-light search-btn" type="submit" name="action">
                <i class="material-icons">send</i>
            </button>
        </div>
    </div>
</div>

<div class="container indigo darken-3 white-text">
    <div class="row">
        <div class="col s6">Content</div>
        <div class="col s2 right">Difficulty</div>
        <div class="col s2 right">Current / Max Players</div>
        <div class="col s2 right">Game Name</div>
    </div>
</div>

{{#each groups as |group| }}

    {{> group}}

{{/each}}